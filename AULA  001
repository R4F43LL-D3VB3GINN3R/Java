15 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
        int numlinhas;
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Quantas linhas deve ter o seu triângulo?: ");
        numlinhas = scanner.nextInt();
        
        if (numlinhas < 0) {

            System.out.println("Insira um número positivo.");

        } else if (numlinhas == 0) {

            System.out.println("Não há triângulo a ser apresentado com zero linhas.");

        } else {
            
            for (int linha = 1; linha <= numlinhas; linha++) {
                
                for (int numero = 1; numero <= linha; numero++) {

                    System.out.print(numero + " ");

                }

                System.out.println();
            }

        }

    }

}

