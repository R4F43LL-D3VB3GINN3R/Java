12 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
    long num;
    
    Scanner scanner = new Scanner(System.in);
    
    System.out.printf("Digite um número: ");
    num = scanner.nextLong();
    
    if (num < 0) {
        
        System.out.println("Insira um número positivo");
        
    } else if (num > 1000000000) {
        
        System.out.println("Número excedeu o tamanho em dígitos");
        
    } else {
        
        if (num < 10) {
            
            System.out.println("O número tem um dígito");
            
        } else if (num < 100) {
            
            System.out.println("O número tem dois dígitos");
            
        } else if (num < 1000) {
            
            System.out.println("O número tem três dígitos");
            
        } else if (num < 10000) {
            
            System.out.println("O número tem quatro dígitos");
            
        } else if (num < 100000) {
            
            System.out.println("O número tem cinco dígitos");
          
        } else if (num < 1000000) {
            
            System.out.println("O número tem seis dígitos");
            
        } else if (num < 10000000) {
            
            System.out.println("O número tem sete dígitos");
            
        } else if (num < 100000000) {
            
            System.out.println("O número tem oito dígitos");
            
        } else if (num < 1000000000) {
            
            System.out.println("O número tem nove dígitos");
            
        } else {
            
            System.out.println("O número tem dez dígitos");
            
        }
        
    }
    
    }
    
}

