11 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
    //11) Crie  um  programa  que  permita  classificar  um  triângulo,  quanto  aos  lados  (escaleno  –  lados  todos diferentes, isósceles – dois lados iguais ou equilátero). 
    
    int lado1;
    int lado2;
    int lado3;
    
    Scanner scanner = new Scanner(System.in);
    
    System.out.printf("Digite o primeiro lado do triângulo: ");
    lado1 = scanner.nextInt();
    
    System.out.printf("Digite o segundo lado do triângulo: ");
    lado2 = scanner.nextInt();
    
    System.out.printf("Digite o terceiro lado do triângulo: ");
    lado3 = scanner.nextInt();
    
    if (lado1 == lado2 && lado1 == lado3) {
        
        System.out.println("O triângulo é Equilátero");
        
    } else if (lado1 == lado2 || lado2 == lado3) {
        
        System.out.println("O triângulo é Isósceles");
        
    } else {
        
        System.out.println("O triângulo é Escaleno");
        
    }
    
    }
    
}

