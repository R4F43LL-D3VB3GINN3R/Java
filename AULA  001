package com.mycompany.demo002;

public class Demo002 {

    public static void main(String[] args) {
        
       int num1 = 2;
       int num2 = 3;
       int aux = 0;
       
       System.out.println("num1 = " + num1);
       System.out.println("num2 = " + num2);
       
       aux = num1;
       num1 = num2;
       num2 = aux;
       
       System.out.println("num1 = " + num1);
       System.out.println("num2 = " + num2);
        
    }
    
}

