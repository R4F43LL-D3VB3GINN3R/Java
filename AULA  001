package com.mycompany.demo002;
import java.text.DecimalFormat;

public class Demo002 {

    public static void main(String[] args) {
        
        double raio = 5;
        double area = 3.14 * raio * raio;
        
        String formatarea = String.format("%.2f", area);
        System.out.println("A área do círculo é: " + formatarea);
        
        double perimetro = (2 * 3.14) * raio;
        
        String formatperimetro = String.format("%.2f", perimetro);
        System.out.println("O períemtro do círculo é: " + formatperimetro);
    }
    
}

