package com.mycompany.demo002;

public class Demo002 {

    public static void main(String[] args) {
        
        char vec[] = { '2', '5' }; 
        
        int num1 = Character.getNumericValue(vec[0]);
        int num2 = Character.getNumericValue(vec[1]);
        int res = num1 + num2;
        
        System.out.println(res);
        
    }
    
}

