10 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
    int ano;
    int mes;
    
    Scanner scanner = new Scanner(System.in);
    
    System.out.printf("Insira o ano: ");
    ano = scanner.nextInt();
    
    System.out.printf("Insira o numero do mês: ");
    mes = scanner.nextInt();
    
    int janeiro = 31;
    int fevereiro = 28;
    int marco = 31;
    int abril = 30;
    int maio = 31;
    int junho = 30;
    int julho = 31;
    int agosto = 31;
    int setembro = 30;
    int outubro = 31;
    int novembro = 30;
    int dezembro = 31;
    
    if (ano % 400 == 0) {
        
        System.out.println("O ano é: " + ano + " e é bissexto.");
        fevereiro = 29;
        
    } else {
        
        System.out.println("O ano é: " + ano);
        
    }
    
    if (mes == 1) {
        
        System.out.println("O mês é Janeiro, e tem " + janeiro + " dias." );
        
    } else if (mes == 2) {
        
        System.out.println("O mês é Fevereiro, e tem " + fevereiro + " dias." );
        
    } else if (mes == 3) {
        
        System.out.println("O mês é Março, e tem " + marco + " dias." );
        
    } else if (mes == 4) {
        
        System.out.println("O mês é Abril, e tem " + abril + " dias." );
        
    } else if (mes == 5) {
        
        System.out.println("O mês é Maio, e tem " + maio + " dias." );
        
    } else if (mes == 6) {
        
        System.out.println("O mês é Junho, e tem " + junho + " dias." );
        
    } else if (mes == 7) {
        
        System.out.println("O mês é Julho, e tem " + julho + " dias." );
        
    } else if (mes == 8) {
        
        System.out.println("O mês é Agosto, e tem " + agosto + " dias." );
        
    } else if (mes == 9) {
        
        System.out.println("O mês é Setembro, e tem " + setembro + " dias." );
        
    } else if (mes == 10) {
        
        System.out.println("O mês é Outubro, e tem " + outubro + " dias." );
        
    } else if (mes == 11) {
        
        System.out.println("O mês é Novembro, e tem " + novembro + " dias." );
        
    } else if (mes == 12) {
        
        System.out.println("O mês é Dezembro, e tem " + dezembro + " dias." );
        
    } else {
        
        System.out.println("Este mês não pertence ao calendário.");
          
    }
    
    }
    
}

