8 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
       int num1;
       int num2;
       int num3;
       
       Scanner scanner = new Scanner(System.in);
       
       System.out.printf("Numero 1: ");
       num1 = scanner.nextInt();
               
       System.out.printf("Numero 2: ");
       num2 = scanner.nextInt();
               
       System.out.printf("Numero 3: "); 
       num3 = scanner.nextInt();
       
       if (num1 > num2 && num2 > num3) {
           
           System.out.println(num1 + " maior do que " + num2 + " e " + num2 + " maior do que " + num3);
           
       } else if (num2 > num1 && num1 > num3) {
        
           System.out.println(num2 + " maior do que " + num1 + " e " + num1 + " maior do que " + num3);  
        
       } else if (num1 == num2 && num1 > num3) {
            
           System.out.println(num1 + " igual a " + num2 + " e " + " maiores do que " + num3);
           
       } else if (num2 == num3 && num2 > num1) {
            
           System.out.println(num2 + " igual a " + num3 + " e " + " maiores do que " + num1);
           
       } else if (num1 == num3 && num1 > num2) {
                  
           System.out.println(num1 + " igual a " + num3 + " e " + " maiores do que " + num2);
           
       } else if (num3 > num2 && num2 > num1) {
                  
           System.out.println(num3 + " maior do que " + num2 + " e " + num2 + " maior do que " + num1);
           
       } else if (num1 == num2 && num3 > num1) {
                  
           System.out.println(num1 + " igual a " + num2 + " e " + num3 + " maior do que " + num1);
           
       } else {
                
           System.out.println("Os três números são iguais");  
           
        }
        
    }
    
}

