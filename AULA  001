13 - 

package com.mycompany.demo002;
import java.util.Scanner;

public class Demo002 {

    public static void main(String[] args) {
        
    int num;
    
    Scanner scanner = new Scanner(System.in);
    
    System.out.printf("Digite um número: ");
    num = scanner.nextInt();
    
    for (int i=0; i<=num; i++) {
        
        if (i % 2 != 0) {
            
            System.out.printf(i + " ");
            
        }     
        
    }
    
    }
    
}

